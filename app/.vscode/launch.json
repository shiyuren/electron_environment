{
    // 调试主进程配置   runtimeExecutable需要指向npm 安装electron的目录
    // 使用 IntelliSense 了解相关属性。 
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Electron Main",
            "runtimeExecutable": "C:\\Users\\Administrator\\AppData\\Roaming\\npm\\electron",
            "windows": {
                "runtimeExecutable": "C:\\Users\\Administrator\\AppData\\Roaming\\npm\\electron.cmd"
            },
            "program": "${workspaceFolder}/app/main.js"
            //"protocol": "legacy"
        }
    ]
}